                    android:scaleType="fitCenter"
                    android:text="16"
                    android:textColor="@color/drive_level_txt_color"
                    android:textSize="@dimen/drive_dirinfo_textsize"
                    android:textStyle="bold" />

                <ImageButton
                    android:id="@+id/btn_zoomout"
                    android:layout_width="@dimen/drive_sdk_mapmode_land_width_height"
