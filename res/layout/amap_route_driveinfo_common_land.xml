            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:layout_width="@dimen/drive_sdk_mapmode_land_width_height"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/btn_zoomin"
                    android:layout_width="@dimen/drive_sdk_mapmode_land_width_height"
                    android:layout_height="@dimen/drive_sdk_mapmode_land_width_height"
                    android:adjustViewBounds="false"
                    android:background="@drawable/selector_bg_zoom_in"
