            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/layout_curpos"
                android:layout_width="@dimen/drive_sdk_mapmode_land_width_height"
                android:layout_height="@dimen/drive_sdk_mapmode_land_width_height"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/btn_1_6_1"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/btn_curpos"
                    android:layout_width="match_parent"
