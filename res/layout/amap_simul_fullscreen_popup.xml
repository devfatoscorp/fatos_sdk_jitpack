<?xml version="1.0" encoding="utf-8"?>
<biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/titlebar_color"
    android:layout_gravity="center_vertical|center_horizontal"
    android:gravity="center_vertical|center_horizontal"
    android:alpha="0.88"
    android:orientation="vertical">


    <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:orientation="horizontal"
            android:layout_gravity="center_vertical|center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/titlebar_color"
            >
            <TextView
                android:text="모의주행 제어"
                android:textColor="@color/LightGoldenrodYellow"
                android:layout_gravity="center_vertical|center_horizontal"
                android:gravity="center_vertical|center_horizontal"
                android:layout_width="match_parent"
                android:textSize = "18dp"
                android:layout_height="40dp"
                android:textAppearance="?android:attr/textAppearanceLarge"/>
        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            >
            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_start_pause"
                android:layout_width="108dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/txt_simul_start_pause"
                    android:layout_width="108dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="일시정지" />
            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_repeat"
                android:layout_width="88dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:layout_marginLeft="5dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="88dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="반복" />
            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_end"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:layout_marginLeft="5dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="140dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="모의주행 종료" />

            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
    </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

    <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
        android:layout_marginTop="7dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:orientation="horizontal"
            android:layout_gravity="center_vertical|center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/titlebar_color"
            >
            <TextView
                android:text="모의주행 속도"
                android:textColor="@color/LightGoldenrodYellow"
                android:layout_gravity="center_vertical|center_horizontal"
                android:gravity="center_vertical|center_horizontal"
                android:layout_width="match_parent"
                android:textSize = "18dp"
                android:layout_height="40dp"
                android:textAppearance="?android:attr/textAppearanceLarge"/>
        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="5dp">

            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_speed_70"
                android:layout_width="108dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:orientation="vertical">
                <TextView
                    android:layout_width="108dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="70km" />

            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_speed_120"
                android:layout_width="108dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round_down"
                android:layout_marginLeft="5dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="108dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="120km" />

            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_simul_speed_200"
                android:layout_width="108dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:layout_marginLeft="5dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="108dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="200km" />

            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
    </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

    <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="5dp">

            <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                android:id="@+id/poplayout_dismiss"
                android:layout_width="108dp"
                android:layout_height="wrap_content"
                android:background="@drawable/popup_round"
                android:orientation="vertical">
                <TextView
                    android:layout_width="108dp"
                    android:layout_height="40dp"
                    android:textSize = "20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="닫기" />

            </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
    </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

</biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>