<?xml version="1.0" encoding="utf-8"?>
<biz.fatossdk.exlib.fatoslayout.AMapSCRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/keyboard_fatos_onehand"
    android:background="@drawable/m_keyboard_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/fmp_text_candidate"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/m_keyboard_textfield"
        android:clickable="true"
        android:gravity="left|center_vertical"
        android:paddingLeft="28dp"
        android:paddingRight="28dp"
        android:singleLine="true"
        android:textColor="@android:color/white"
        android:textSize="@dimen/fmp_text_recentinput" />

    <biz.fatossdk.keyboard.GenericKeyboardView
        android:id="@+id/keyboardview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="false"
        android:layout_below="@+id/fmp_text_candidate"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginRight="23dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:keyBackground="@drawable/key_selector"
        android:keyTextColor="@android:color/black"
        android:keyTextSize="25sp"
        android:visibility="gone" />
</biz.fatossdk.exlib.fatoslayout.AMapSCRelativeLayout>
