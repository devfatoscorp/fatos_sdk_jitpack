                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:textAllCaps="false"
                            android:background="@drawable/evsearch_normal_btn"
                            />
                        <Button
                            android:id="@+id/ev_ac3_btn"
                            android:text="@string/string_acph"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:textAllCaps="false"
                            android:background="@drawable/evsearch_normal_btn"
                            />
                        <Button
                            android:id="@+id/ev_charge_option_btn"
                            android:text="@string/string_moderate"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:textAllCaps="false"
                            android:background="@drawable/evsearch_normal_btn"
                            />

                    </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>
                    <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                        android:layout_marginTop="4dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@color/titlebar_color">
                        <biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
                            android:orientation="horizontal"
                            android:layout_width="22dp"
                            android:layout_height="match_parent"
                            android:background="@color/titlebar_color">
                            <ImageView
                                android:layout_width="11dp"
                                android:layout_height="34dp"
                                android:src = "@drawable/icon_ev_filter"
                                android:layout_gravity="center_vertical|center_horizontal"
                                />
                        </biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout>

                        <Button
                            android:id="@+id/ev_charge_free_btn"
                            android:text="@string/string_free"
                            android:textColor="@color/white"
                            android:textSize="15dp"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:textAllCaps="false"
                            android:background="@drawable/evsearch_normal_btn"
                            />
