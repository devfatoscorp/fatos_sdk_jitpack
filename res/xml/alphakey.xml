<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="25%p"
    >

    <Row>
        <Key android:codes="49" android:keyIcon="@drawable/m_keyboard_eng_b_01" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyIcon="@drawable/m_keyboard_eng_s_05"/>
        <Key android:codes="51" android:keyIcon="@drawable/m_keyboard_eng_s_07"/>
        <Key android:codes="-5" android:keyIcon="@drawable/m_keyboard_icon_delete_nor"
             android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="52" android:keyIcon="@drawable/m_keyboard_eng_s_13" android:keyEdgeFlags="left"/>
        <Key android:codes="53" android:keyIcon="@drawable/m_keyboard_eng_s_14"/>
        <Key android:codes="54" android:keyIcon="@drawable/m_keyboard_eng_s_17"/>
        <Key android:codes="-2" android:keyIcon="@drawable/m_keyboard_number_out"
            android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="55" android:keyIcon="@drawable/m_keyboard_eng_s_22" android:keyEdgeFlags="left"/>
        <Key android:codes="56" android:keyIcon="@drawable/m_keyboard_eng_s_24"/>
        <Key android:codes="57" android:keyIcon="@drawable/m_keyboard_eng_s_26"/>
        <Key android:codes="-6" android:keyIcon="@drawable/m_keyboard_eng_s_in"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="45" android:keyIcon="@drawable/m_keyboard_icon_hyphen" android:keyEdgeFlags="left"/>
        <Key android:codes="-1" android:keyIcon="@drawable/m_keyboard_shift_out" android:isSticky="true"/>
        <Key android:codes="32" android:keyIcon="@drawable/m_keyboard_icon_space"/>
        <Key android:codes="-4" android:keyIcon="@drawable/m_keyboard_icon_search_nor"
            android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    