<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="25%p"
    >

    <Row>
        <Key android:codes="49" android:keyIcon="@drawable/m_keyboard_number_01" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyIcon="@drawable/m_keyboard_number_02"/>
        <Key android:codes="51" android:keyIcon="@drawable/m_keyboard_number_03"/>
        <Key android:codes="-5" android:keyIcon="@drawable/m_keyboard_icon_delete_nor"
             android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="52" android:keyIcon="@drawable/m_keyboard_number_04" android:keyEdgeFlags="left"/>
        <Key android:codes="53" android:keyIcon="@drawable/m_keyboard_number_05"/>
        <Key android:codes="54" android:keyIcon="@drawable/m_keyboard_number_06"/>
        <Key android:codes="-2" android:keyIcon="@drawable/m_keyboard_number_in"
            android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="55" android:keyIcon="@drawable/m_keyboard_number_07" android:keyEdgeFlags="left"/>
        <Key android:codes="56" android:keyIcon="@drawable/m_keyboard_number_08"/>
        <Key android:codes="57" android:keyIcon="@drawable/m_keyboard_number_09"/>
        <Key android:codes="-6" android:keyIcon="@drawable/m_keyboard_number_11"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="45" android:keyIcon="@drawable/m_keyboard_icon_hyphen" android:keyEdgeFlags="left"/>
        <Key android:codes="48" android:keyIcon="@drawable/m_keyboard_number_10"/>
        <Key android:codes="32" android:keyIcon="@drawable/m_keyboard_icon_space"/>
        <Key android:codes="-4" android:keyIcon="@drawable/m_keyboard_icon_search_nor"
            android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    