<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="25%p"
    >

    <Row>
        <Key android:codes="49" android:keyIcon="@drawable/m_keyboard_kor_01" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyIcon="@drawable/m_keyboard_kor_02"/>
        <Key android:codes="51" android:keyIcon="@drawable/m_keyboard_kor_03"/>
        <Key android:codes="-5" android:keyIcon="@drawable/m_keyboard_icon_delete_nor"
             android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="52" android:keyIcon="@drawable/m_keyboard_kor_04" android:keyEdgeFlags="left"/>
        <Key android:codes="53" android:keyIcon="@drawable/m_keyboard_kor_05"/>
        <Key android:codes="54" android:keyIcon="@drawable/m_keyboard_kor_06"/>
        <Key android:codes="-2" android:keyIcon="@drawable/m_keyboard_number_out"
            android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="55" android:keyIcon="@drawable/m_keyboard_kor_07" android:keyEdgeFlags="left"/>
        <Key android:codes="56" android:keyIcon="@drawable/m_keyboard_kor_08"/>
        <Key android:codes="57" android:keyIcon="@drawable/m_keyboard_kor_09"/>
        <Key android:codes="-6" android:keyIcon="@drawable/m_keyboard_kor_in"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="46" android:keyIcon="@drawable/m_keyboard_kor_11" android:keyEdgeFlags="left"/>
        <Key android:codes="48" android:keyIcon="@drawable/m_keyboard_kor_10"/>
        <Key android:codes="44" android:keyIcon="@drawable/m_keyboard_kor_12"/>
        <Key android:codes="-4" android:keyIcon="@drawable/m_keyboard_icon_search_nor"
            android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    