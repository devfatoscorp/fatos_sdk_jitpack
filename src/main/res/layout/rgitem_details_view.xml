	<ImageView
		android:id="@+id/tbtimg"
		android:layout_width="40dp"
		android:layout_height="50dp"
		android:layout_marginLeft="2dp"
		android:paddingRight="5dp"
		android:paddingTop="1dp" />

	<biz.fatossdk.exlib.fatoslayout.AMapSCLinearLayout
